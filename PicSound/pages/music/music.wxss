.whole{
  background-color: #fce9e9;
}

.search_bar {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin: 20rpx 20rpx;
  height: 100rpx;
  background-color: white;
  border-radius: 50rpx;
}
.search_bar .search_bar_txt{
  position: relative;
  top: 30%;
  left: 5%;
}
.search_bar .search_bar_logo{
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 100%;
  width: 100rpx;
  background-color: rgb(203, 203, 248);
  border-radius: 0 50% 50% 0;
}
.search_bar .search_bar_img{
  padding-left: 15%;
  height: 60rpx;
  width: 60rpx;
}

.whole .item{
  display: flex;
  padding: 10rpx 0rpx 10rpx 32rpx;
  justify-content: space-between;
  border-bottom: 1rpx solid #ccc;
  margin: 0 32rpx;
}

.item .txtbox{
  display: flex;
  flex-direction: column;
}

.item .playimg{
  height: 80rpx;
  width: 80rpx;
}

.item .txtbox .mname{
  font-size: 35rpx;
}
.item .txtbox .name{
  font-size: 28rpx;
  color: #777;
}